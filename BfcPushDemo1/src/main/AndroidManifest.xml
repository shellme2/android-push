<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.eebbk.bfc.demo.push_1">

    <application
        android:name=".PushTestApplication"
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="SYNC_APPKEY"
            android:value="9e57050646b64b32bf0cc1195fb99db5" />
        <meta-data
            android:name="SYNC_RID_TAG"
            android:value="2"/>

        <receiver android:name=".MessageReceiver">
            <intent-filter>
                <action android:name="com.eebbk.bfc.im.sync_response" />
                <category android:name="com.eebbk.bfc.demo.push_1"/>
            </intent-filter>
        </receiver>

    </application>

</manifest>